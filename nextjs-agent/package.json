{
  "name": "nextjs-agent",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack -p 5000",
    "build": "next build --turbopack",
    "start": "next start -p 5000",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "prepare": "husky",
    "upload-docs": "tsx --env-file=.env scripts/upload-documents.ts",
    "test-qdrant": "tsx --env-file=.env scripts/test-qdrant-connection.ts",
    "create-index": "tsx --env-file=.env scripts/create-qdrant-index.ts"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js,jsx,json,css,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@langchain/anthropic": "^0.3.12",
    "@langchain/core": "^0.3.29",
    "@langchain/openai": "^1.0.0",
    "@qdrant/js-client-rest": "^1.15.1",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@xenova/transformers": "^2.17.2",
    "ajv": "^8.17.1",
    "langchain": "^0.3.14",
    "marked": "^16.4.1",
    "next": "15.5.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-markdown": "^10.1.0",
    "react-syntax-highlighter": "^16.1.0",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/pdf-parse": "^1.1.5",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "canvas": "^3.2.0",
    "dotenv": "^17.2.3",
    "eslint": "^9",
    "eslint-config-next": "15.5.6",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.5",
    "mammoth": "^1.11.0",
    "pdf-parse": "^2.4.5",
    "pdfjs-dist": "^5.4.394",
    "prettier": "^3.6.2",
    "tailwindcss": "^4",
    "tesseract.js": "^6.0.1",
    "tsx": "^4.20.6",
    "typescript": "^5"
  }
}
